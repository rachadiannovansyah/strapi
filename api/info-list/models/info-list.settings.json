{
  "kind": "collectionType",
  "collectionName": "info_lists",
  "info": {
    "name": "Information-list",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "photos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "showTime": {
      "type": "time",
      "private": true,
      "default": "00:00:00"
    },
    "admin_user": {
      "plugin": "admin",
      "model": "user"
    }
  }
}
