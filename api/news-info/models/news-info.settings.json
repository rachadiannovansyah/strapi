{
  "kind": "collectionType",
  "collectionName": "news_infos",
  "info": {
    "name": "news-list",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "photo": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "videos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "sourceNews": {
      "type": "richtext",
      "required": true
    },
    "isPublish": {
      "type": "boolean",
      "required": true
    },
    "published": {
      "type": "datetime"
    },
    "isHighlight": {
      "type": "boolean"
    },
    "admin_user": {
      "plugin": "admin",
      "model": "user"
    }
  }
}
